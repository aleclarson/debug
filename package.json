{
  "name": "@wbe/debug",
  "version": "0.1.0",
  "description": "Tiny debug tool for terminal and browsers",
  "author": "Willy Brauner",
  "license": "MIT",
  "types": "dist/index",
  "source": "src/index.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "private": true,
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/willybrauner/libraries.git"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
    "compile": "microbundle",
    "dev": "microbundle --watch",
    "dev:browser": "vite",
    "dev:node": "DEBUG=true node example/node/index.js",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.1",
    "jest": "^27.0.6",
    "microbundle": "^0.13.3",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5",
    "vite": "^2.5.1"
  },
  "keywords": [
    "debug"
  ],
  "publishConfig": {
    "access": "public"
  }
}
